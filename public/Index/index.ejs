  width: 100%;

}

.footer .footer-newsletter .campo {
  
  width: 100%;
  height: 45px;
  float: left;
  border: 1px solid white;
  background: #dddddd;
  margin-bottom: 15px;
  border-radius: 6px;
  outline: none;
  padding: 5px 10px;
  color: #cccccc;

}

.footer .footer-newsletter .campo:focus {
  
