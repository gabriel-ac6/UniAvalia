<script>
  function salvarDadosLocalmente() {
    // Recupere os valores do nome e do e-mail do formulário
    var nome = document.getElementById('nome').value;
    var email = document.getElementById('email').value;

    // Verifique se os campos foram preenchidos
    if (nome && email) {
      // Salve os dados localmente usando localStorage
      localStorage.setItem('nome', nome);
      localStorage.setItem('email', email);

      // Você pode adicionar uma mensagem de confirmação ou redirecionar o usuário aqui
    } else {
      // Caso os campos não estejam preenchidos, você pode exibir uma mensagem de erro
      alert('Por favor, preencha todos os campos');
    }
  }
